<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <smo-custom j:dependencies="bootstrap4-components bootstrap4-core cloudinary-integration default"
                  j:modulePriority="0"
                  j:moduleType="module"
                  j:title="smo-custom"
                  jcr:mixinTypes="jmix:hasExternalProviderExtension"
                  jcr:primaryType="jnt:module">
         <sources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-smo-custom-1.0-SNAPSHOT">
            <pom.xml jcr:primaryType="jnt:xmlFile"/>
            <src jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                 jcr:primaryType="jnt:folder">
               <main jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                     jcr:primaryType="jnt:folder">
                  <resources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                             jcr:primaryType="jnt:folder">
                     <jnt_mainContent jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                      jcr:primaryType="jnt:nodeTypeFolder">
                        <html jcr:mixinTypes="jmix:hasExternalProviderExtension"
                              jcr:primaryType="jnt:templateTypeFolder">
                           <mainContent.card.jsp jcr:primaryType="jnt:viewFile"/>
                        </html>
                     </jnt_mainContent>
                     <META-INF jcr:mixinTypes="jmix:hasExternalProviderExtension"
                               jcr:primaryType="jnt:metaInfFolder">
                        <definitions.cnd jcr:primaryType="jnt:definitionFile"/>
                     </META-INF>
                     <jnt_news jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                               jcr:primaryType="jnt:nodeTypeFolder"
                               scmStatus="modified">
                        <html jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                              jcr:primaryType="jnt:templateTypeFolder"
                              scmStatus="modified">
                           <news.card-img-bottomtext.jsp jcr:mixinTypes="jmix:sourceControl"
                                                         jcr:primaryType="jnt:viewFile"
                                                         scmStatus="added"/>
                           <news.card.jsp jcr:mixinTypes="jmix:sourceControl"
                                          jcr:primaryType="jnt:viewFile"
                                          scmStatus="added"/>
                           <news.card-fulltext.jsp jcr:mixinTypes="jmix:sourceControl"
                                                   jcr:primaryType="jnt:viewFile"
                                                   scmStatus="added"/>
                        </html>
                     </jnt_news>
                  </resources>
               </main>
            </src>
         </sources>
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
